-- MySQL
docker run --name mysql -e MYSQL_ROOT_PASSWORD=123456 -d mysql

-- RabbitMQ
docker run -d --hostname localhost --name rabbit13 -p 15672:15672 -p 5672:5672 -p 25676:25676 rabbitmq:3-management

-- API
docker build -t devambev-api .
docker run --name devambev-api -p 8080:80 -d devambev-api

-- FRONT
docker build -t devambev.front .
docker run --name devambev-front -p 3000:3000 -d devambev.front